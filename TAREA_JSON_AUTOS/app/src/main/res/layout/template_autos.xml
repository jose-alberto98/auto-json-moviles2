<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp"
    android:clickable="true"
    android:foreground="?attr/selectableItemBackground"
    >

    <LinearLayout
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="id"
            android:textSize="12dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="modelo"
            android:textSize="12dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="marca"
            android:textSize="12dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="color"
            android:textSize="12dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="cilindro"
            android:textSize="12dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="115dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="estado"
            android:textSize="12dp"
            android:textStyle="bold"
            android:typeface="normal" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/mostrar_id"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="mostrar_id"
            android:textSize="12.3dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_modelo"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="mostrar_modelo"
            android:textSize="12.3dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_marca"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="mostrar_marca"
            android:textSize="12.3dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_color"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="mostrar_color"
            android:textSize="12.3dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_cilindro"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="mostrar_cilindro"
            android:textSize="12.3dp"
            android:textStyle="bold"
            android:typeface="normal" />

        <TextView
            android:id="@+id/mostrar_estado"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="serif"
            android:text="mostrar_estado"
            android:textSize="12.3dp"
            android:textStyle="bold"
            android:typeface="normal" />

    </LinearLayout>

</LinearLayout>